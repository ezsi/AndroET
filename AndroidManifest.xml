<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      
      package="org.measureit.androet"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-permission android:name="android.permission.INTERNET" /> 
    <application android:label="@string/app_name" android:icon="@drawable/icon" android:debuggable="true">
        <activity android:name="AccountsActivity"
                  android:label="AndroET">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Preferences" android:label="Settings"/>
        <activity android:name="AccountActivity" android:label="Edit account"/>
        <activity android:name="TransferActivity" android:windowSoftInputMode="stateAlwaysVisible" android:label="Transfer money"/>
        <activity android:name="GroupActivity" android:label="Edit group"/>
        <activity android:name="TransactionsActivity" android:label="Transactions"/>
        <activity android:name="SummaryActivity" android:label="Account summary"/>
        <activity android:name="TransactionActivity" android:windowSoftInputMode="stateAlwaysVisible" android:label="Edit transaction"/>
    </application>
</manifest> 
